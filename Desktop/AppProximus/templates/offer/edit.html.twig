										{{ form_widget(form.client.carteIdentite, {'attr': {'class': 'form-control form-control-solid form-control-lg', 'placeholder': 'Carte Identité'}}) }}
										<span class="form-text text-muted">Please enter your Postcode.</span>
									</div>
									<!--end::Input-->
								</div>
								<div
									class="col-xl-4">
									<!--begin::Input-->
									<div class="form-group">
										<label>Lien Drive Partageable</label>
										{{ form_widget(form.client.lienDrivePartageable, {'attr': {'class': 'form-control form-control-solid form-control-lg', 'placeholder': 'Lien Drive Partageable'}}) }}
										<span class="form-text text-muted">Please enter your Postcode.</span>
									</div>
									<!--end::Input-->
								</div>
							</div>

							<div class="separator separator-dashed my-5"></div>

							<h3 class="mb-10 font-weight-bold text-dark">Informations Produits</h3>

							{% set var = [
								{
									'id' : '1',
									'designation' :'Données Mobile',
									'quantite' :'12'
								}, {
									'id' :'2',
									'designation' :'Internet Mobile',
									'quantite' :'15'
								}, {
									'id' :'3',
									'designation' :'Box Internet',
									'quantite' :'18'
								}, {
									'id' :'4',
									'designation' :'Carte SIM',
									'quantite' :'2'
								}
							] %}

							{% for categorie in categories %}
								{% if categorie.produits %}
									<h6 class="mb-10 font-weight-bold text-dark mx-10">[Catégorie] {{ categorie.nom }}</h6>
									{% for produit in var %}
										<div class="row mx-5">
											<div
												class="col-xl-1 form-group align-middle">
												<!--begin::Input-->
												<div class="checkbox-list">
													<label class="checkbox checkbox-lg">
														<input type="checkbox" name="produits_checkbox[]" value="{{ produit.id }}"/>
														<span></span>
													</label>
												</div>
												<!--end::Input-->
											</div>
											<div
												class="col-xl-8">
												<!--begin::Input-->
												<div class="form-group">
													<input type="text" disabled="true" class="form-control form-control-solid form-control-lg" placeholder="Produit" value="{{ produit.designation }}"/>
													<span class="form-text text-muted">Please enter your Postcode.</span>
												</div>
												<!--end::Input-->
											</div>
											<div
												class="col-xl-3">
												<!--begin::Input-->
												<div class="form-group">
													<input name="quantite[]" type="number" class="form-control form-control-solid form-control-lg" placeholder="Quantité"/>
													<span class="form-text text-muted">Please enter your Postcode.</span>
												</div>
												<!--end::Input-->
											</div>
										</div>
									{% endfor %}
								{% endif %}
							{% endfor %}

						</div>
						<!--end::Wizard Step 1-->

						{{ form_widget(form) }}
					</div>
					<!--end::Body-->

					<!--begin::Footer-->
					<div class="card-footer py-5 px-5">
						<div class="card-toolbar float-right">
							<a href="#" class="btn btn-primary font-weight-bolder px-5 mr-3">
								<i class="fas fa-save"></i>
								Enregistrer & Valider
							</a>
							<a href="#" class="btn btn-light-primary font-weight-bolder px-5 mr-3">
								Enregistrer
							</a>
							<a href="#" class="btn btn-light btn-text-primary btn-hover-text-primary font-weight-bold px-5 mr-3">
								Annuler
							</a>
						</div>
					</div>
					<!--end::Footer-->
				</div>
				{{ form_end(form) }}
			</div>
			<!--end::Container-->
		</div>
		<!--end::Entry-->
	</div>
	<!--end::Content-->
{% endblock %}
